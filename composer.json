        }
    ],
    "version": "2.2.2",
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "psr/log": "^1.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "ext-xml": "*",
        "phpunit/phpunit": "^4.8.35 || ^5.7",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "YooKassaPayout\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\YooKassaPayout\\": "tests/"
        }
